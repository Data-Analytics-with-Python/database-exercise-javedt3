      "source": [
        "# ===============================\n",
        "# 2017 NHL Goalies — Colab Script\n",
        "# ===============================\n",
        "\n",
        "# 0) Imports\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "pd.set_option(\"display.max_columns\", 200)\n",
        "pd.set_option(\"display.width\", 200)\n",
        "\n",
        "# -------------------------------\n",
        "# 1) SETTINGS — EDIT IF NEEDED\n",
        "# -------------------------------\n",
        "FILE = \"/content/nhl_draft_analysi_08_08 MASTER (1).xlsx\"  # uploaded path\n",
        "SHEET = \"2017 MASTER GOAL\"                                  # your sheet name\n",
        "YEAR = 2017                                                 # target draft year\n",
        "PICK_TO_LOOKUP = 15                                         # change this to test a specific overall pick\n",
        "\n",
        "# If your file is in /content, leave as-is. Otherwise, set the correct path above.\n",
        "\n",
        "# -------------------------------\n",
        "# 2) LOAD DATA\n",
        "# -------------------------------\n",
        "df = pd.read_excel(FILE, sheet_name=SHEET)\n",
        "\n",
        "# -------------------------------\n",
        "# 3) COLUMN RESOLUTION (robust)\n",
        "# We resolve key columns by common names; adjust here if your headers differ.\n",
        "# -------------------------------\n",
        "def find_col(cols, candidates):\n",
        "    \"\"\"Return the first matching column name from candidates (case-insensitive).\"\"\"\n",
        "    cl = {c.lower(): c for c in cols}\n",
        "    for cand in candidates:\n",
        "        if cand.lower() in cl:\n",
        "            return cl[cand.lower()]\n",
        "    # fuzzy: try contains\n",
        "    for c in cols:\n",
        "        lc = c.lower()\n",
        "        for cand in candidates:\n",
        "            if cand.lower() in lc:\n",
        "                return c\n",
        "    return None\n",
        "\n",
        "col_player = find_col(df.columns, [\"Player\",\"Name\"])\n",
        "col_team   = find_col(df.columns, [\"Team\"])\n",
        "col_pos    = find_col(df.columns, [\"Pos\",\"Position\"])\n",
        "col_over   = find_col(df.columns, [\"Overall\",\"Overall Pick\",\"Pick\"])\n",
        "col_year   = find_col(df.columns, [\"draft_year\",\"Year\"])\n",
        "\n",
        "# Your notes said: PS=column Z, PERF=column AP, Assessment=column AW (Excel letters).\n",
        "# In pandas we use headers, so resolve by expected header names:\n",
        "col_ps     = find_col(df.columns, [\"PS\",\"Point Shares\"])\n",
        "col_perf   = find_col(df.columns, [\"Performance_Index_0_100\",\"PERF\",\"Performance Index\"])\n",
        "col_assess = find_col(df.columns, [\"Statistical_Draft_Assessment\",\"Assessment\"])\n",
        "\n",
        "# Games played for goalies — try common names used in your code/data\n",
        "col_gpg    = find_col(df.columns, [\"GS_GP\",\"Goalie_GP\",\"GP\",\"Games\",\"Games Played\"])\n",
        "\n",
        "# Sanity print (uncomment to inspect)\n",
        "#print(col_player, col_team, col_pos, col_over, col_year, col_ps, col_perf, col_assess, col_gpg)\n",
        "\n",
        "# Basic checks\n",
        "required = [col_player, col_team, col_pos, col_over, col_ps, col_perf, col_assess]\n",
        "missing  = [n for n,v in zip(\n",
        "    [\"player\",\"team\",\"pos\",\"overall\",\"ps\",\"perf\",\"assessment\"],\n",
        "    required\n",
        ") if v is None]\n",
        "if missing:\n",
        "    raise ValueError(f\"Missing required columns (by header): {missing}\\n\"\n",
        "                     f\"Available columns: {list(df.columns)}\")\n",
        "\n",
        "# -------------------------------\n",
        "# 4) FILTER TO 2017 & GOALIES\n",
        "# -------------------------------\n",
        "df_2017 = df.copy()\n",
        "if col_year and col_year in df_2017.columns:\n",
        "    df_2017 = df_2017[df_2017[col_year] == YEAR].copy()\n",
        "\n",
        "# goalie mask: match 'G' or 'Goalie' or values starting with G (robust to spaces)\n",
        "pos_series = df_2017[col_pos].astype(str).str.strip().str.lower()\n",
        "goalie_mask = (pos_series.eq(\"g\") | pos_series.eq(\"goalie\") | pos_series.str.startswith(\"g\"))\n",
        "g = df_2017[goalie_mask].copy()\n",
        "\n",
        "# If games column is missing, create a safe 0 column to avoid errors in buckets\n",
        "if col_gpg is None:\n",
        "    g[\"_GP_\"] = 0\n",
        "    gp_col = \"_GP_\"\n",
        "else:\n",
        "    gp_col = col_gpg\n",
        "\n",
        "# Ensure numeric\n",
        "for c in [col_ps, col_perf, gp_col, col_over]:\n",
        "    g[c] = pd.to_numeric(g[c], errors=\"coerce\")\n",
        "\n",
        "# -------------------------------\n",
        "# 5) COMPUTE “OPTIMAL PICK” & DRAFT DIFFERENCE (optional, in case needed)\n",
        "# If your sheet already has these, you can skip; otherwise:\n",
        "# -------------------------------\n",
        "g = g.sort_values(col_perf, ascending=False).copy()\n",
        "g[\"Optimal_Pick_Statistical\"] = np.arange(1, len(g)+1)\n",
        "g[\"Statistical_Draft_Difference\"] = g[col_over] - g[\"Optimal_Pick_Statistical\"]\n",
        "\n",
        "# -------------------------------\n",
        "# 6) GAMES BUCKETS & COUNTS\n",
        "# Buckets: 0, 1–50, 51–100, 100–150, 150–200, 200+\n",
        "# -------------------------------\n",
        "bins   = [-0.1, 0, 50, 100, 150, 200, np.inf]\n",
        "labels = [\"0\",\"1-50\",\"51-100\",\"100-150\",\"150-200\",\"200+\"]\n",
        "g[\"GP_Bucket\"] = pd.cut(g[gp_col].fillna(0), bins=bins, labels=labels)\n",
        "\n",
        "games_counts = (\n",
        "    g[\"GP_Bucket\"].value_counts()\n",
        "    .reindex(labels)\n",
        "    .rename_axis(\"Games_Bucket\")\n",
        "    .reset_index(name=\"Goalies_Count\")\n",
        ")\n",
        "total_goalies = len(g)\n",
        "\n",
        "# -------------------------------\n",
        "# 7) STANDARDS COUNTS\n",
        "# Assessment text (e.g., Exceeds / Meets / Does Not Meet / Not Enough Data)\n",
        "# -------------------------------\n",
        "assess_series = g[col_assess].astype(str).str.strip()\n",
        "\n",
        "# Normalize typical labels\n",
        "mapping = {\n",
        "    \"does not meet expectations\":\"Does Not Meet Expectations\",\n",
        "    \"meets expectation\":\"Meets Expectation\",\n",
        "    \"exceeds expectation\":\"Exceeds Expectation\",\n",
        "    \"not enough data\":\"Not Enough Data\",\n",
        "}\n",
        "assess_norm = assess_series.str.lower().map(mapping).fillna(assess_series)\n",
        "standards_counts = assess_norm.value_counts().reindex(\n",
        "    [\"Does Not Meet Expectations\",\"Meets Expectation\",\"Exceeds Expectation\",\"Not Enough Data\"]\n",
        ").fillna(0).astype(int)\n",
        "\n",
        "# -------------------------------\n",
        "# 8) TOP GOALIES TABLE\n",
        "# -------------------------------\n",
        "top_goalies = g[[col_player, col_team, col_pos, col_over, col_ps, col_perf, col_assess,\n",
        "                 \"Optimal_Pick_Statistical\",\"Statistical_Draft_Difference\"]].copy()\n",
        "top_goalies = top_goalies.sort_values(col_perf, ascending=False).head(15).reset_index(drop=True)\n",
        "top_goalies.columns = [\"Player\",\"Team\",\"Pos\",\"Overall\",\"PS\",\"PERF(0-100)\",\"Assessment\",\n",
        "                       \"Optimal_Pick\",\"Draft_Diff(Overall-Optimal)\"]\n",
        "\n",
        "# -------------------------------\n",
        "# 9) PICK LOOKUP (enter Overall in PICK_TO_LOOKUP)\n",
        "# -------------------------------\n",
        "pick_row = g.loc[(g[col_over] == PICK_TO_LOOKUP)].copy()\n",
        "# If multiple goalies share same overall (shouldn't), keep first\n",
        "pick_info = (pick_row.head(1)[[col_player, col_team, col_pos, col_over, col_ps, col_perf, col_assess,\n",
        "                               \"Optimal_Pick_Statistical\",\"Statistical_Draft_Difference\"]]\n",
        "             .rename(columns={col_player:\"Player\", col_team:\"Team\", col_pos:\"Pos\",\n",
        "                              col_over:\"Overall\", col_ps:\"PS\", col_perf:\"PERF(0-100)\",\n",
        "                              col_assess:\"Assessment\",\n",
        "                              \"Optimal_Pick_Statistical\":\"Optimal_Pick\",\n",
        "                              \"Statistical_Draft_Difference\":\"Draft_Diff(Overall-Optimal)\"}))\n",
        "\n",
        "# -------------------------------\n",
        "# 10) PRINT / DISPLAY\n",
        "# -------------------------------\n",
        "print(\"=== 2017 NHL DRAFT — GOALIES ONLY ===\")\n",
        "print(f\"Total goalies: {total_goalies}\\n\")\n",
        "\n",
        "print(\"GAMES BUCKETS (Goalies):\")\n",
        "display(games_counts)\n",
        "\n",
        "print(\"\\nSTANDARDS COUNTS (Goalies):\")\n",
        "display(pd.DataFrame({\"Assessment\":[\"Does Not Meet Expectations\",\"Meets Expectation\",\"Exceeds Expectation\",\"Not Enough Data\"],\n",
        "                      \"Count\": standards_counts.values}))\n",
        "\n",
        "print(\"\\nTOP 15 GOALIES BY PERF (0–100):\")\n",
        "display(top_goalies)\n",
        "\n",
        "print(f\"\\nLOOKUP — Overall Pick = {PICK_TO_LOOKUP}\")\n",
        "if pick_info.empty:\n",
        "    print(\"No goalie at that overall pick (or not in this sheet).\")\n",
        "else:\n",
        "    display(pick_info)\n",
        "\n",
        "# -------------------------------\n",
        "# 11) OPTIONAL: PLOTS\n",
        "# -------------------------------\n",
        "# A) Pick vs Performance (goalies)\n",
        "plt.figure()\n",
        "plt.scatter(g[col_over], g[col_perf])\n",
        "plt.gca().invert_xaxis()\n",
        "plt.title(\"2017 Goalies: Draft Pick vs Performance Index\")\n",
        "plt.xlabel(\"Overall Pick (lower = earlier)\")\n",
        "plt.ylabel(\"Performance Index (0–100)\")\n",
        "plt.show()\n",
        "\n",
        "# B) Standards bar\n",
        "cnt = standards_counts.fillna(0).astype(int)\n",
        "plt.figure()\n",
        "plt.bar(cnt.index, cnt.values)\n",
        "plt.title(\"2017 Goalies: Assessment Counts\")\n",
        "plt.xticks(rotation=20)\n",
        "plt.ylabel(\"Count\")\n",
        "plt.show()\n",
        "\n",
        "# -------------------------------\n",
        "# 12) EXPORTS\n",
        "# -------------------------------\n",
        "with pd.ExcelWriter(\"/content/Goalie_2017_Analysis.xlsx\") as xw:\n",
        "    g.to_excel(xw, index=False, sheet_name=\"Goalies_2017_All\")\n",
        "    games_counts.to_excel(xw, index=False, sheet_name=\"Games_Buckets\")\n",
        "    pd.DataFrame({\"Assessment\":cnt.index, \"Count\":cnt.values}).to_excel(xw, index=False, sheet_name=\"Standards_Counts\")\n",
        "    top_goalies.to_excel(xw, index=False, sheet_name=\"Top15\")\n",
        "    if not pick_info.empty:\n",
        "        pick_info.to_excel(xw, index=False, sheet_name=\"Pick_Lookup\")\n",
        "\n",
        "print(\"\\nSaved file: /content/Goalie_2017_Analysis.xlsx\")\n"
      ]
    }
  ]
}